#include <iostream>
#include <queue>

using namespace std;

in main() {
    queue<int> fila;
    
    fila.push(1);
    fila.push(2);
    fila.push(3);
  
    cout << "tamanho da fila: " << fila.size() << endl;
    cout << "Primeiro elmento da fila: " << fila.fron() << endl;
    
    fila.pop();
    
    cout << "Novo primeiro elemento da fila: << fila.front() << endl;
    
    return 0;
}
